{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 80,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^react-dom",
    "^react-router",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@types$",
    "^@types/(.*)$",
    "^@/(.*)$",
    "^@components/(.*)$",
    "^@services/(.*)$",
    "^@hooks/(.*)$",
    "^@routes/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0"
}
